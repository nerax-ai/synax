{
  "name": "synax-ai",
  "version": "0.0.1",
  "type": "module",
  "bin": {
    "synax": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
